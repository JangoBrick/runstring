{
  "name": "runstring",
  "version": "1.1.0",
  "description": "Convert JS functions to runnable strings. With parameter serialization!",
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "lint": "ts-standard",
    "lint-fix": "ts-standard --fix",
    "test": "npm run clean && ts-mocha --recursive test/**/*.test.js",
    "coverage": "nyc --reporter=lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/runstring.git"
  },
  "keywords": [
    "functions",
    "string",
    "stringify",
    "runnable",
    "eval",
    "executable",
    "invoke"
  ],
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/runstring/issues"
  },
  "homepage": "https://github.com/meyfa/runstring",
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "mocha": "^7.1.1",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "ts-mocha": "^8.0.0",
    "ts-standard": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
